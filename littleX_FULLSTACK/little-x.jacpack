{
  "name": "little-x",
  "description": "Little X social app template",
  "config": {
    "project": {
      "name": "little-x",
      "version": "1.0.0",
      "description": "Little X - A Twitter-like social app built with Jac",
      "entry-point": "main.jac"
    },
    "dependencies": {
      "numpy": ">=1.24.0",
      "scikit-learn": ">=1.3.0",
      "npm": {
        "jac-client-node": "1.0.4",
        "lucide-react": "^0.469.0",
        "dev": {
          "@jac-client/dev-deps": "1.0.0"
        }
      }
    },
    "dev-dependencies": {
      "watchdog": ">=3.0.0"
    },
    "serve": {
      "base_route_app": "app"
    },
    "plugins": {
      "client": {
        "app_meta_data": {
          "title": "Little-X",
          "description": "A decentralized social platform built with Jac",
          "keywords": "little-x social jac jaseci",
          "author": "Jaseci Labs",
          "theme_color": "#f97316",
          "icon": "/static/assets/littlex-logo.png"
        }
      }
    }
  },
  "files": {
    "frontend.cl.jac": "\"\"\"Little X - Frontend declarations.\"\"\"\n\nimport from \"@jac/runtime\" { jacSignup, jacLogin, jacLogout, jacIsLoggedIn }\nsv import from .server { setup_profile, get_profile, get_all_profiles, follow_user, unfollow_user, create_tweet, delete_tweet, like_tweet, add_comment, load_feed, get_trending }\nimport from \"lucide-react\" { Home, Compass, User, LogOut, Search, Feather }\nimport from .components.TweetCard { TweetCard }\nimport from .components.AuthForm { AuthForm }\nimport \"./global.css\";\n\ndef:pub app() -> Any {\n    has isLoggedIn: bool = False,\n        checkingAuth: bool = True,\n        isSignup: bool = False,\n        authUsername: str = \"\",\n        authPassword: str = \"\",\n        authError: str = \"\",\n        authLoading: bool = False,\n        activeTab: str = \"feed\",\n        myProfile: Any = None,\n        tweets: list = [],\n        feedLoading: bool = True,\n        composerText: str = \"\",\n        searchQuery: str = \"\",\n        allProfiles: list = [],\n        editingBio: bool = False,\n        bioText: str = \"\",\n        trendingTags: list = [],\n        notifCount: int = 0,\n        showWelcome: bool = False;\n\n    can with entry {\n        isLoggedIn = jacIsLoggedIn();\n        checkingAuth = False;\n    }\n\n    can with [isLoggedIn] entry {\n        if isLoggedIn {\n            showWelcome = not localStorage.getItem(\"lx_welcomed\");\n            loadData();\n        }\n    }\n\n    async def loadData() -> None;\n    async def handleLogin() -> None;\n    async def handleSignup() -> None;\n    def handleLogout() -> None;\n    async def postTweet() -> None;\n    async def handleLike(tweetId: str) -> None;\n    async def handleComment(tweetId: str, content: str) -> None;\n    async def handleDelete(tweetId: str) -> None;\n    async def handleFollow(profileId: str) -> None;\n    async def handleUnfollow(profileId: str) -> None;\n    async def handleSaveBio() -> None;\n    async def handleSearch(query: str) -> None;\n    def handleRepost(tweetId: str, content: str, author: str) -> None;\n    def handleDismissWelcome() -> None;\n\n    if checkingAuth {\n        return <div className=\"lx-loading\">Loading...</div>;\n    }\n\n    if not isLoggedIn {\n        return <AuthForm\n            isSignup={isSignup}\n            username={authUsername}\n            password={authPassword}\n            error={authError}\n            loading={authLoading}\n            onUsernameChange={lambda e: Any -> None { authUsername = e.target.value; }}\n            onPasswordChange={lambda e: Any -> None { authPassword = e.target.value; }}\n            onSubmit={lambda e: Any -> None { e.preventDefault(); if isSignup { handleSignup(); } else { handleLogin(); } }}\n            onToggleMode={lambda -> None { isSignup = not isSignup; authError = \"\"; }}\n        />;\n    }\n\n    profileUsername = myProfile.username if myProfile else \"\";\n    displayUsername = profileUsername.split(\"@\")[0] if profileUsername.includes(\"@\") else profileUsername;\n    profileAvatar = \"https://i.pravatar.cc/150?u=fake@\" + profileUsername;\n    followingIds = [f[\"id\"] for f in myProfile.following] if myProfile and myProfile.following else [];\n    charsLeft = 280 - composerText.length;\n    ring_progress = Math.min(1.0, (280 - charsLeft) / 280.0);\n    ring_offset = String(62.83 * (1.0 - ring_progress));\n    ring_color = \"#ef4444\" if charsLeft < 0 else (\"#f97316\" if charsLeft < 50 else (\"#fb923c\" if charsLeft < 100 else \"#2a2a35\"));\n    toFollow = [u for u in allProfiles if not followingIds.includes(u[\"id\"]) and u[\"id\"] != (myProfile.id if myProfile else \"\")];\n    current_total_activity = (myProfile.tweets if myProfile and myProfile.tweets else []).reduce(lambda acc, t: acc + t[\"likes\"].length + t[\"comments\"].length, 0);\n\n    return <div className=\"lx-root\">\n        <aside className=\"lx-sidebar-left\">\n            <div className=\"lx-brand\">\n                <img src=\"/static/assets/littlex-logo.png\" className=\"lx-brand-logo\" alt=\"little-x\" />\n                <span className=\"lx-brand-name\">LITTLE-X</span>\n            </div>\n\n            <nav className=\"lx-nav\">\n                <div\n                    className={\"lx-nav-item\" + (\" lx-nav-item-active\" if activeTab == \"feed\" else \"\")}\n                    onClick={lambda -> None { activeTab = \"feed\"; }}\n                >\n                    <Home size={22} />\n                    <span>Home</span>\n                </div>\n                <div\n                    className={\"lx-nav-item\" + (\" lx-nav-item-active\" if activeTab == \"explore\" else \"\")}\n                    onClick={lambda -> None { activeTab = \"explore\"; }}\n                >\n                    <Compass size={22} />\n                    <span>Explore</span>\n                </div>\n                <div\n                    className={\"lx-nav-item\" + (\" lx-nav-item-active\" if activeTab == \"profile\" else \"\")}\n                    onClick={lambda -> None { activeTab = \"profile\"; localStorage.setItem(\"lx_activity_total\", String(current_total_activity)); notifCount = 0; }}\n                >\n                    <User size={22} />\n                    <span>Profile</span>\n                    {(\n                        <span className=\"lx-nav-badge\">{String(notifCount)}</span>\n                    ) if notifCount > 0 else None}\n                </div>\n            </nav>\n\n            <button className=\"lx-compose-btn\" onClick={lambda -> None { activeTab = \"feed\"; }}>\n                <Feather size={18} />\n                <span>Post</span>\n            </button>\n\n            <div style={{\"flex\": \"1\"}} />\n\n            <div className=\"lx-sidebar-user\">\n                <img className=\"lx-avatar\" src={profileAvatar} alt={profileUsername} />\n                <div className=\"lx-sidebar-user-info\">\n                    <div className=\"lx-sidebar-user-name\">{displayUsername}</div>\n                    <div className=\"lx-sidebar-user-handle\">{\"@\" + displayUsername}</div>\n                </div>\n                <button className=\"lx-logout-icon-btn\" onClick={lambda -> None { handleLogout(); }}>\n                    <LogOut size={18} />\n                </button>\n            </div>\n        </aside>\n\n        <main className=\"lx-main\">\n            <div className=\"lx-feed-header\">\n                <span className=\"lx-feed-title\">\n                    {(\"Home\" if activeTab == \"feed\" else (\"Explore\" if activeTab == \"explore\" else \"Profile\"))}\n                </span>\n                {(\n                    <button\n                        className=\"lx-edit-profile-btn\"\n                        onClick={lambda -> None { editingBio = not editingBio; bioText = (myProfile.bio if myProfile and myProfile.bio else \"\"); }}\n                    >\n                        {\"Cancel\" if editingBio else \"Edit profile\"}\n                    </button>\n                ) if activeTab == \"profile\" else None}\n            </div>\n\n            {(\n                <div>\n                    {(\n                        <div className=\"lx-welcome-banner\">\n                            <div className=\"lx-welcome-content\">\n                                <div className=\"lx-welcome-title\">Welcome to LITTLE-X</div>\n                                <div className=\"lx-welcome-tips\">\n                                    <span className=\"lx-welcome-tip\">{\"Post a thought to get started\"}</span>\n                                    <span className=\"lx-welcome-tip\">{\"Use #hashtags to join trending topics\"}</span>\n                                    <span className=\"lx-welcome-tip\">{\"Explore and follow people in Explore\"}</span>\n                                </div>\n                            </div>\n                            <button className=\"lx-welcome-dismiss\" onClick={lambda -> None { handleDismissWelcome(); }}>\n                                {\"Got it\"}\n                            </button>\n                        </div>\n                    ) if showWelcome else None}\n                    <div className=\"lx-composer\">\n                        <img className=\"lx-avatar\" src={profileAvatar} alt={profileUsername} />\n                        <div className=\"lx-composer-body\">\n                            <textarea\n                                className=\"lx-composer-textarea\"\n                                placeholder=\"What's happening?\"\n                                value={composerText}\n                                onChange={lambda e: Any -> None { composerText = e.target.value; }}\n                                rows={3}\n                            />\n                            <div className=\"lx-composer-footer\">\n                                <div className=\"lx-char-ring-wrap\">\n                                    <svg className=\"lx-char-ring\" viewBox=\"0 0 24 24\">\n                                        <circle className=\"lx-char-ring-bg\" cx={12} cy={12} r={10} />\n                                        <circle\n                                            className=\"lx-char-ring-fill\"\n                                            cx={12}\n                                            cy={12}\n                                            r={10}\n                                            strokeDasharray={\"62.83\"}\n                                            strokeDashoffset={ring_offset}\n                                            stroke={ring_color}\n                                        />\n                                    </svg>\n                                    {(\n                                        <span className={\"lx-char-count-over\" if charsLeft < 0 else \"lx-char-count-warn\"}>\n                                            {String(charsLeft)}\n                                        </span>\n                                    ) if charsLeft <= 20 else None}\n                                </div>\n                                <button\n                                    className=\"lx-post-btn\"\n                                    disabled={not composerText.trim() or charsLeft < 0}\n                                    onClick={lambda -> None { postTweet(); }}\n                                >\n                                    Post\n                                </button>\n                            </div>\n                        </div>\n                    </div>\n\n                    {(\n                        <div className=\"lx-loading\">Loading feed...</div>\n                    ) if feedLoading else None}\n\n                    {(\n                        <div>\n                            {(\n                                <div className=\"lx-empty\">\n                                    <div className=\"lx-empty-title\">Nothing here yet</div>\n                                    <span>Follow people or post something!</span>\n                                </div>\n                            ) if tweets.length == 0 else None}\n                            {[\n                                <TweetCard\n                                    key={tweet[\"id\"]}\n                                    tweet={tweet}\n                                    myUsername={profileUsername}\n                                    onLike={handleLike}\n                                    onComment={handleComment}\n                                    onDelete={handleDelete}\n                                    onRepost={handleRepost}\n                                /> for tweet in tweets\n                            ]}\n                        </div>\n                    ) if not feedLoading else None}\n                </div>\n            ) if activeTab == \"feed\" else None}\n\n            {(\n                <div>\n                    {(\n                        <div className=\"lx-loading\">Loading users...</div>\n                    ) if not allProfiles else None}\n                    {[\n                        <div className=\"lx-explore-item\" key={u[\"id\"]}>\n                            <img className=\"lx-avatar\" src={\"https://i.pravatar.cc/150?u=fake@\" + u[\"username\"]} alt={u[\"username\"]} />\n                            <div className=\"lx-explore-info\">\n                                <div className=\"lx-explore-name\">{u[\"username\"].split(\"@\")[0] if u[\"username\"].includes(\"@\") else u[\"username\"]}</div>\n                                <div className=\"lx-explore-handle\">{\"@\" + (u[\"username\"].split(\"@\")[0] if u[\"username\"].includes(\"@\") else u[\"username\"])}</div>\n                                <div className=\"lx-explore-bio\">{u[\"bio\"] if u[\"bio\"] else \"No bio yet\"}</div>\n                            </div>\n                            {(\n                                <button\n                                    className=\"lx-unfollow-btn\"\n                                    onClick={lambda -> None { handleUnfollow(u[\"id\"]); }}\n                                >\n                                    Following\n                                </button>\n                            ) if followingIds.includes(u[\"id\"]) else (\n                                <button\n                                    className=\"lx-follow-btn\"\n                                    onClick={lambda -> None { handleFollow(u[\"id\"]); }}\n                                >\n                                    Follow\n                                </button>\n                            ) if u[\"id\"] != (myProfile.id if myProfile else \"\") else None}\n                        </div>\n                        for u in allProfiles\n                    ]}\n                </div>\n            ) if activeTab == \"explore\" else None}\n\n            {(\n                <div>\n                    <div className=\"lx-profile-banner\" />\n                    <div className=\"lx-profile-info\">\n                        <div className=\"lx-profile-avatar-wrap\">\n                            <img className=\"lx-avatar-lg\" src={profileAvatar} alt={profileUsername} />\n                        </div>\n                        <div className=\"lx-profile-display-name\">{displayUsername}</div>\n                        <div className=\"lx-profile-handle\">{\"@\" + displayUsername}</div>\n                        {(\n                            <div className=\"lx-profile-bio-text\">\n                                {myProfile.bio if myProfile and myProfile.bio else \"No bio yet\"}\n                            </div>\n                        ) if not editingBio else None}\n                        {(\n                            <div className=\"lx-bio-edit-wrap\">\n                                <textarea\n                                    className=\"lx-bio-input\"\n                                    rows={3}\n                                    placeholder=\"Write your bio...\"\n                                    value={bioText}\n                                    onChange={lambda e: Any -> None { bioText = e.target.value; }}\n                                />\n                                <div style={{\"display\": \"flex\", \"gap\": \"0.5rem\"}}>\n                                    <button className=\"lx-bio-save-btn\" onClick={lambda -> None { handleSaveBio(); }}>\n                                        Save\n                                    </button>\n                                    <button\n                                        className=\"lx-edit-profile-btn\"\n                                        onClick={lambda -> None { editingBio = False; }}\n                                    >\n                                        Cancel\n                                    </button>\n                                </div>\n                            </div>\n                        ) if editingBio else None}\n                        <div className=\"lx-profile-stats\">\n                            <div className=\"lx-profile-stat\">\n                                <strong>{String(len(myProfile.following) if myProfile and myProfile.following else 0)}</strong>\n                                Following\n                            </div>\n                            <div className=\"lx-profile-stat\">\n                                <strong>{String(len(myProfile.followers) if myProfile and myProfile.followers else 0)}</strong>\n                                Followers\n                            </div>\n                            <div className=\"lx-profile-stat\">\n                                <strong>{String(len(myProfile.tweets) if myProfile and myProfile.tweets else 0)}</strong>\n                                Posts\n                            </div>\n                        </div>\n                    </div>\n\n                    {[\n                        <TweetCard\n                            key={tweet[\"id\"]}\n                            tweet={tweet}\n                            myUsername={profileUsername}\n                            onLike={handleLike}\n                            onComment={handleComment}\n                            onDelete={handleDelete}\n                            onRepost={handleRepost}\n                        /> for tweet in (myProfile.tweets if myProfile and myProfile.tweets else [])\n                    ]}\n                </div>\n            ) if activeTab == \"profile\" else None}\n        </main>\n\n        <aside className=\"lx-sidebar-right\">\n            <div className=\"lx-sidebar-right-inner\">\n                <div className=\"lx-search-wrap\">\n                    <span className=\"lx-search-icon\">\n                        <Search size={16} />\n                    </span>\n                    <input\n                        className=\"lx-search-input\"\n                        placeholder=\"Search Little-X\"\n                        value={searchQuery}\n                        onChange={lambda e: Any -> None { searchQuery = e.target.value; }}\n                        onKeyDown={lambda e: Any -> None { if e.key == \"Enter\" { handleSearch(e.target.value); } }}\n                    />\n                    <button\n                        className=\"lx-search-btn\"\n                        onClick={lambda -> None { handleSearch(searchQuery); }}\n                    >\n                        Search\n                    </button>\n                </div>\n\n                {(\n                    <div className=\"lx-widget\">\n                        <div className=\"lx-widget-title\">Trending</div>\n                        {[\n                            <div\n                                className=\"lx-trend-item\"\n                                key={t[\"tag\"]}\n                                onClick={lambda -> None { searchQuery = t[\"tag\"]; handleSearch(t[\"tag\"]); activeTab = \"feed\"; }}\n                            >\n                                <span className=\"lx-trend-tag\">{t[\"tag\"]}</span>\n                                <span className=\"lx-trend-count\">{String(t[\"count\"]) + \" posts\"}</span>\n                            </div>\n                            for t in trendingTags\n                        ]}\n                    </div>\n                ) if trendingTags.length > 0 else None}\n\n                {(\n                    <div className=\"lx-widget\">\n                        <div className=\"lx-widget-title\">Who to follow</div>\n                        {[\n                            <div className=\"lx-follow-item\" key={u[\"id\"]}>\n                                <img className=\"lx-avatar-sm\" src={\"https://i.pravatar.cc/150?u=fake@\" + u[\"username\"]} alt={u[\"username\"]} />\n                                <div className=\"lx-follow-item-info\">\n                                    <div className=\"lx-follow-item-name\">{u[\"username\"].split(\"@\")[0] if u[\"username\"].includes(\"@\") else u[\"username\"]}</div>\n                                    <div className=\"lx-follow-item-handle\">{\"@\" + (u[\"username\"].split(\"@\")[0] if u[\"username\"].includes(\"@\") else u[\"username\"])}</div>\n                                </div>\n                                <button\n                                    className=\"lx-follow-btn\"\n                                    onClick={lambda -> None { handleFollow(u[\"id\"]); }}\n                                >\n                                    Follow\n                                </button>\n                            </div>\n                            for u in toFollow.slice(0, 5)\n                        ]}\n                        {(\n                            <div className=\"lx-widget-show-more\" onClick={lambda -> None { activeTab = \"explore\"; }}>\n                                Show more\n                            </div>\n                        ) if toFollow.length > 5 else None}\n                    </div>\n                ) if toFollow.length > 0 else None}\n\n                <div className=\"lx-about-widget\">\n                    <div className=\"lx-about-title\">LITTLE-X</div>\n                    <div className=\"lx-about-desc\">A decentralized social platform built with Jac. Share thoughts, follow people, join conversations.</div>\n                    <div className=\"lx-about-links\">\n                        <span className=\"lx-about-tag\">{\"#jacdev\"}</span>\n                        <span className=\"lx-about-tag\">{\"#jaseci\"}</span>\n                        <span className=\"lx-about-tag\">{\"#open-source\"}</span>\n                    </div>\n                </div>\n            </div>\n        </aside>\n\n        <nav className=\"lx-mobile-nav\">\n            <div\n                className={\"lx-mobile-nav-item\" + (\" lx-mobile-nav-item-active\" if activeTab == \"feed\" else \"\")}\n                onClick={lambda -> None { activeTab = \"feed\"; }}\n            >\n                <Home size={24} />\n            </div>\n            <div\n                className={\"lx-mobile-nav-item\" + (\" lx-mobile-nav-item-active\" if activeTab == \"explore\" else \"\")}\n                onClick={lambda -> None { activeTab = \"explore\"; }}\n            >\n                <Compass size={24} />\n            </div>\n            <div\n                className={\"lx-mobile-nav-item\" + (\" lx-mobile-nav-item-active\" if activeTab == \"profile\" else \"\")}\n                onClick={lambda -> None { activeTab = \"profile\"; localStorage.setItem(\"lx_activity_total\", String(current_total_activity)); notifCount = 0; }}\n            >\n                <User size={24} />\n                {(\n                    <span className=\"lx-nav-badge\">{String(notifCount)}</span>\n                ) if notifCount > 0 else None}\n            </div>\n        </nav>\n\n        <button className=\"lx-mobile-fab\" onClick={lambda -> None { activeTab = \"feed\"; }}>\n            <Feather size={22} />\n        </button>\n    </div>;\n}\n",
    "server.jac": "\"\"\"Little X - Social graph backend with TF-IDF feed ranking.\"\"\"\n\nimport datetime;\nimport from sklearn.feature_extraction.text { TfidfVectorizer }\nimport from sklearn.metrics.pairwise { cosine_similarity }\n\nglob _vectorizer: Any = TfidfVectorizer();\n\ndef _compute_sim(query: str, content: str) -> float {\n    if not query.strip() {\n        return 1.0;\n    }\n    try {\n        t = _vectorizer.fit_transform([query, content]);\n        return float(cosine_similarity(t[0:1], t[1:2])[0][0]);\n    } except Exception {\n        return 0.0;\n    }\n}\n\ndef _now() -> str {\n    return datetime.datetime.utcnow().isoformat() + \"Z\";\n}\n\nnode Profile {\n    has username: str = \"\";\n    has bio: str = \"\";\n    has created_at: str = \"\";\n}\n\nnode Tweet {\n    has content: str = \"\";\n    has author_username: str = \"\";\n    has created_at: str = \"\";\n    has likes: list = [];\n    has comments: list = [];\n}\n\nedge Follow {}\nedge Post {}\n\nwalker setup_profile {\n    has username: str = \"\";\n    has bio: str = \"\";\n\n    can run with Root entry {\n        profiles = [-->(?:Profile)];\n        if not profiles {\n            p = here ++> Profile(username=self.username, bio=self.bio, created_at=_now());\n            grant(p[0], level=ConnectPerm);\n            report {\"id\": jid(p[0]), \"username\": p[0].username, \"bio\": p[0].bio, \"created_at\": p[0].created_at};\n        } else {\n            profile = profiles[0];\n            if self.username {\n                profile.username = self.username;\n            }\n            if self.bio {\n                profile.bio = self.bio;\n            }\n            report {\"id\": jid(profile), \"username\": profile.username, \"bio\": profile.bio, \"created_at\": profile.created_at};\n        }\n    }\n}\n\nwalker get_profile {\n    can run with Root entry {\n        profiles = [-->(?:Profile)];\n        if not profiles {\n            report {\"error\": \"No profile found\"};\n            disengage;\n        }\n        p = profiles[0];\n        my_id = jid(p);\n\n        followers: list = [];\n        following: list = [];\n\n        for r in allroots() {\n            other_list = [r-->(?:Profile)];\n            if other_list {\n                other = other_list[0];\n                if jid(other) != my_id {\n                    follow_out = [edge other ->:Follow:-> p];\n                    if follow_out {\n                        followers.append({\"id\": jid(other), \"username\": other.username});\n                    }\n                    follow_in = [edge p ->:Follow:-> other];\n                    if follow_in {\n                        following.append({\"id\": jid(other), \"username\": other.username});\n                    }\n                }\n            }\n        }\n\n        tweets = [\n            {\n                \"id\": jid(t),\n                \"content\": t.content,\n                \"author_username\": t.author_username,\n                \"created_at\": t.created_at,\n                \"likes\": t.likes,\n                \"comments\": t.comments\n            }\n            for t in [p-->(?:Tweet)]\n        ];\n        tweets.sort(key=lambda t: t[\"created_at\"], reverse=True);\n        report {\n            \"id\": my_id,\n            \"username\": p.username,\n            \"bio\": p.bio,\n            \"followers\": followers,\n            \"following\": following,\n            \"tweets\": tweets\n        };\n    }\n}\n\nwalker:pub get_all_profiles {\n    can run with Root entry {\n        results: list = [];\n        for r in allroots() {\n            prof = [r-->(?:Profile)];\n            if prof {\n                results.append({\"id\": jid(prof[0]), \"username\": prof[0].username, \"bio\": prof[0].bio});\n            }\n        }\n        report results;\n    }\n}\n\nwalker follow_user {\n    has target_id: str;\n\n    can run with Root entry {\n        my_profiles = [-->(?:Profile)];\n        if not my_profiles {\n            report {\"error\": \"No profile\"};\n            disengage;\n        }\n        my_profile = my_profiles[0];\n        for r in allroots() {\n            target_list = [r-->(?:Profile)];\n            if target_list and jid(target_list[0]) == self.target_id {\n                my_profile +>:Follow():+> target_list[0];\n                report {\"success\": True};\n                disengage;\n            }\n        }\n        report {\"error\": \"User not found\"};\n    }\n}\n\nwalker unfollow_user {\n    has target_id: str;\n\n    can run with Root entry {\n        my_profiles = [-->(?:Profile)];\n        if not my_profiles {\n            report {\"error\": \"No profile\"};\n            disengage;\n        }\n        my_profile = my_profiles[0];\n        for r in allroots() {\n            target_list = [r-->(?:Profile)];\n            if target_list and jid(target_list[0]) == self.target_id {\n                follow_edges = [edge my_profile ->:Follow:-> target_list[0]];\n                if follow_edges {\n                    del follow_edges[0];\n                }\n                report {\"success\": True};\n                disengage;\n            }\n        }\n        report {\"error\": \"User not found\"};\n    }\n}\n\nwalker create_tweet {\n    has content: str;\n\n    can run with Root entry {\n        profiles = [-->(?:Profile)];\n        if not profiles {\n            report {\"error\": \"No profile\"};\n            disengage;\n        }\n        profile = profiles[0];\n        tweet = profile +>:Post():+> Tweet(\n            content=self.content,\n            author_username=profile.username,\n            created_at=_now(),\n            likes=[],\n            comments=[]\n        );\n        grant(tweet[0], level=ConnectPerm);\n        t = tweet[0];\n        report {\"id\": jid(t), \"content\": t.content, \"author_username\": t.author_username, \"created_at\": t.created_at, \"likes\": t.likes, \"comments\": t.comments};\n    }\n}\n\nwalker delete_tweet {\n    has tweet_id: str;\n\n    can run with Root entry {\n        profiles = [-->(?:Profile)];\n        if not profiles {\n            report {\"error\": \"No profile\"};\n            disengage;\n        }\n        profile = profiles[0];\n        for tweet in [profile-->(?:Tweet)] {\n            if jid(tweet) == self.tweet_id {\n                del tweet;\n                report {\"success\": True};\n                disengage;\n            }\n        }\n        report {\"error\": \"Tweet not found\"};\n    }\n}\n\nwalker like_tweet {\n    has tweet_id: str;\n\n    can run with Root entry {\n        my_profiles = [-->(?:Profile)];\n        if not my_profiles {\n            report {\"error\": \"No profile\"};\n            disengage;\n        }\n        username = my_profiles[0].username;\n        for r in allroots() {\n            prof_list = [r-->(?:Profile)];\n            if prof_list {\n                for tweet in [prof_list[0]-->(?:Tweet)] {\n                    if jid(tweet) == self.tweet_id {\n                        if username in tweet.likes {\n                            tweet.likes = [u for u in tweet.likes if u != username];\n                            report {\"liked\": False, \"likes\": tweet.likes};\n                        } else {\n                            tweet.likes = tweet.likes + [username];\n                            report {\"liked\": True, \"likes\": tweet.likes};\n                        }\n                        disengage;\n                    }\n                }\n            }\n        }\n        report {\"error\": \"Tweet not found\"};\n    }\n}\n\nwalker add_comment {\n    has tweet_id: str;\n    has content: str;\n\n    can run with Root entry {\n        my_profiles = [-->(?:Profile)];\n        if not my_profiles {\n            report {\"error\": \"No profile\"};\n            disengage;\n        }\n        username = my_profiles[0].username;\n        for r in allroots() {\n            prof_list = [r-->(?:Profile)];\n            if prof_list {\n                for tweet in [prof_list[0]-->(?:Tweet)] {\n                    if jid(tweet) == self.tweet_id {\n                        comment = {\"username\": username, \"content\": self.content, \"created_at\": _now()};\n                        tweet.comments = tweet.comments + [comment];\n                        report {\"success\": True, \"comment\": comment};\n                        disengage;\n                    }\n                }\n            }\n        }\n        report {\"error\": \"Tweet not found\"};\n    }\n}\n\nwalker get_trending {\n    can run with Root entry {\n        tag_counts: dict = {};\n        for r in allroots() {\n            prof = [r-->(?:Profile)];\n            if prof {\n                for tweet in [prof[0]-->(?:Tweet)] {\n                    for word in tweet.content.split() {\n                        if word.startswith(\"#\") and len(word) > 1 {\n                            tag = word.lower().rstrip(\".,!?;:\");\n                            if tag in tag_counts {\n                                tag_counts[tag] += 1;\n                            } else {\n                                tag_counts[tag] = 1;\n                            }\n                        }\n                    }\n                }\n            }\n        }\n        sorted_tags = sorted(tag_counts.items(), key=lambda x: x[1], reverse=True);\n        report [{\"tag\": t[0], \"count\": t[1]} for t in sorted_tags[:8]];\n    }\n}\n\nwalker load_feed {\n    has search_query: str = \"\";\n\n    can run with Root entry {\n        my_profiles = [-->(?:Profile)];\n        if not my_profiles {\n            report [];\n            disengage;\n        }\n        my_profile = my_profiles[0];\n        all_tweets: list = [];\n\n        for tweet in [my_profile-->(?:Tweet)] {\n            all_tweets.append({\n                \"id\": jid(tweet),\n                \"content\": tweet.content,\n                \"author_username\": tweet.author_username,\n                \"created_at\": tweet.created_at,\n                \"likes\": tweet.likes,\n                \"comments\": tweet.comments,\n                \"is_mine\": True\n            });\n        }\n\n        for followed in [my_profile->:Follow:->(?:Profile)] {\n            for tweet in [followed-->(?:Tweet)] {\n                all_tweets.append({\n                    \"id\": jid(tweet),\n                    \"content\": tweet.content,\n                    \"author_username\": tweet.author_username,\n                    \"created_at\": tweet.created_at,\n                    \"likes\": tweet.likes,\n                    \"comments\": tweet.comments,\n                    \"is_mine\": False\n                });\n            }\n        }\n\n        if self.search_query {\n            q = self.search_query.lower().strip();\n            all_tweets = [t for t in all_tweets if q in t[\"content\"].lower()];\n            all_tweets.sort(key=lambda t: t[\"created_at\"], reverse=True);\n        } else {\n            all_tweets.sort(key=lambda t: t[\"created_at\"], reverse=True);\n        }\n\n        report all_tweets;\n    }\n}\n",
    "main.jac": "\"\"\"Little X - Entry point combining backend and frontend.\"\"\"\n\n\nimport from server {\n        setup_profile,\n        get_profile,\n        get_all_profiles,\n        follow_user,\n        unfollow_user,\n        create_tweet,\n        delete_tweet,\n        like_tweet,\n        add_comment,\n        load_feed,\n        get_trending\n    }\n\n\ncl {\n    import from .frontend { app as ClientApp }\n\n    def:pub app() -> Any {\n        return <ClientApp />;\n    }\n}\n",
    "README.md": "# little-x\n\nA Jac client-side application with React support.\n\n## Project Structure\n\n```\nlittle-x/\n\u251c\u2500\u2500 jac.toml              # Project configuration\n\u251c\u2500\u2500 main.jac              # Main application entry\n\u251c\u2500\u2500 components/           # Reusable components\n\u2502   \u2514\u2500\u2500 Button.cl.jac     # Example Jac component\n\u251c\u2500\u2500 assets/               # Static assets (images, fonts, etc.)\n\u2514\u2500\u2500 build/                # Build output (generated)\n```\n\n## Getting Started\n\nStart the development server:\n\n```bash\njac start main.jac\n```\n\n## Components\n\nCreate Jac components in `components/` as `.cl.jac` files and import them:\n\n```jac\ncl import from .components.Button { Button }\n```\n\n## Adding Dependencies\n\nAdd npm packages with the --cl flag:\n\n```bash\njac add --cl react-router-dom\n```\n",
    "frontend.impl.jac": "\"\"\"Little X - Frontend function implementations.\"\"\"\n\nimpl app.loadData -> None {\n    result = root spawn setup_profile(username=localStorage.getItem(\"lx_username\") or \"\");\n    myProfile = result.reports[0] if result.reports else None;\n\n    feedResult = root spawn load_feed(search_query=\"\");\n    tweets = feedResult.reports[0] if feedResult.reports else [];\n    feedLoading = False;\n\n    profileResult = root spawn get_profile();\n    myProfile = profileResult.reports[0] if profileResult.reports else myProfile;\n\n    profilesResult = root spawn get_all_profiles();\n    allProfiles = profilesResult.reports[0] if profilesResult.reports else [];\n\n    trendResult = root spawn get_trending();\n    trendingTags = trendResult.reports[0] if trendResult.reports else [];\n\n    total_activity = (myProfile.tweets if myProfile and myProfile.tweets else []).reduce(lambda acc, t: acc + t[\"likes\"].length + t[\"comments\"].length, 0);\n    last_seen_str = localStorage.getItem(\"lx_activity_total\");\n    last_seen = parseInt(last_seen_str) if last_seen_str else 0;\n    notifCount = total_activity - last_seen if total_activity > last_seen else 0;\n}\n\nimpl app.handleLogin -> None {\n    authError = \"\";\n    if not authUsername.trim() or not authPassword {\n        authError = \"Please fill in all fields\";\n        return;\n    }\n    authLoading = True;\n    success = await jacLogin(authUsername, authPassword);\n    authLoading = False;\n    if success {\n        localStorage.setItem(\"lx_username\", authUsername);\n        isLoggedIn = True;\n        authPassword = \"\";\n        authError = \"\";\n    } else {\n        authError = \"Invalid username or password\";\n    }\n}\n\nimpl app.handleSignup -> None {\n    authError = \"\";\n    if not authUsername.trim() or not authPassword {\n        authError = \"Please fill in all fields\";\n        return;\n    }\n    if authPassword.length < 4 {\n        authError = \"Password must be at least 4 characters\";\n        return;\n    }\n    authLoading = True;\n    result = await jacSignup(authUsername, authPassword);\n    authLoading = False;\n    if result[\"success\"] {\n        localStorage.setItem(\"lx_username\", authUsername);\n        isLoggedIn = True;\n        authPassword = \"\";\n        authError = \"\";\n    } else {\n        authError = result[\"error\"] if result[\"error\"] else \"Signup failed\";\n    }\n}\n\nimpl app.handleLogout -> None {\n    jacLogout();\n    localStorage.removeItem(\"lx_username\");\n    isLoggedIn = False;\n    myProfile = None;\n    tweets = [];\n    allProfiles = [];\n    activeTab = \"feed\";\n    authUsername = \"\";\n    authPassword = \"\";\n}\n\nimpl app.postTweet -> None {\n    if not composerText.trim() {\n        return;\n    }\n    text = composerText;\n    composerText = \"\";\n    root spawn create_tweet(content=text);\n    feedResult = root spawn load_feed(search_query=\"\");\n    tweets = feedResult.reports[0] if feedResult.reports else [];\n    profileResult = root spawn get_profile();\n    myProfile = profileResult.reports[0] if profileResult.reports else myProfile;\n}\n\nimpl app.handleLike(tweetId: str) -> None {\n    root spawn like_tweet(tweet_id=tweetId);\n    feedResult = root spawn load_feed(search_query=searchQuery);\n    tweets = feedResult.reports[0] if feedResult.reports else [];\n    if myProfile {\n        profileResult = root spawn get_profile();\n        myProfile = profileResult.reports[0] if profileResult.reports else myProfile;\n    }\n}\n\nimpl app.handleComment(tweetId: str, content: str) -> None {\n    root spawn add_comment(tweet_id=tweetId, content=content);\n    feedResult = root spawn load_feed(search_query=searchQuery);\n    tweets = feedResult.reports[0] if feedResult.reports else [];\n}\n\nimpl app.handleDelete(tweetId: str) -> None {\n    root spawn delete_tweet(tweet_id=tweetId);\n    feedResult = root spawn load_feed(search_query=searchQuery);\n    tweets = feedResult.reports[0] if feedResult.reports else [];\n    profileResult = root spawn get_profile();\n    myProfile = profileResult.reports[0] if profileResult.reports else myProfile;\n}\n\nimpl app.handleFollow(profileId: str) -> None {\n    if profileId == \"_load\" {\n        profilesResult = root spawn get_all_profiles();\n        allProfiles = profilesResult.reports[0] if profilesResult.reports else [];\n        return;\n    }\n    root spawn follow_user(target_id=profileId);\n    profileResult = root spawn get_profile();\n    myProfile = profileResult.reports[0] if profileResult.reports else myProfile;\n    feedResult = root spawn load_feed(search_query=searchQuery);\n    tweets = feedResult.reports[0] if feedResult.reports else [];\n}\n\nimpl app.handleUnfollow(profileId: str) -> None {\n    root spawn unfollow_user(target_id=profileId);\n    profileResult = root spawn get_profile();\n    myProfile = profileResult.reports[0] if profileResult.reports else myProfile;\n    feedResult = root spawn load_feed(search_query=searchQuery);\n    tweets = feedResult.reports[0] if feedResult.reports else [];\n}\n\nimpl app.handleSaveBio -> None {\n    uname = myProfile.username if myProfile else \"\";\n    root spawn setup_profile(username=uname, bio=bioText);\n    profileResult = root spawn get_profile();\n    myProfile = profileResult.reports[0] if profileResult.reports else myProfile;\n    editingBio = False;\n}\n\nimpl app.handleSearch(query: str) -> None {\n    feedResult = root spawn load_feed(search_query=query);\n    tweets = feedResult.reports[0] if feedResult.reports else [];\n}\n\nimpl app.handleRepost(tweetId: str, content: str, author: str) -> None {\n    display_author = author.split(\"@\")[0] if author.includes(\"@\") else author;\n    composerText = \"RT @\" + display_author + \": \" + content;\n    activeTab = \"feed\";\n}\n\nimpl app.handleDismissWelcome -> None {\n    localStorage.setItem(\"lx_welcomed\", \"1\");\n    showWelcome = False;\n}\n",
    "global.css": "@import url(\"https://fonts.googleapis.com/css2?family=Poppins:wght@700;800;900&family=Plus+Jakarta+Sans:ital,wght@0,400;0,500;0,600;0,700;0,800;1,400&display=swap\");\n\n/* \u2500\u2500 Design Tokens \u2500\u2500 */\n\n:root {\n  /* Surface Hierarchy */\n  --lx-bg: #060607;\n  --lx-bg-subtle: #0c0c0e;\n  --lx-bg-card: #111115;\n  --lx-bg-raised: #17171c;\n  --lx-bg-input: #141418;\n  --lx-bg-hover: #0e0e11;\n\n  /* Borders */\n  --lx-border: rgba(255, 255, 255, 0.07);\n  --lx-border-subtle: rgba(255, 255, 255, 0.04);\n  --lx-border-strong: rgba(255, 255, 255, 0.13);\n\n  /* Text */\n  --lx-text: #eeeef2;\n  --lx-text-secondary: #9898a6;\n  --lx-text-muted: #52525e;\n\n  /* Accent \u2014 Orange */\n  --lx-accent: #f97316;\n  --lx-accent-dark: #c2550d;\n  --lx-accent-light: #fb923c;\n  --lx-accent-glow: rgba(249, 115, 22, 0.1);\n  --lx-accent-glow2: rgba(249, 115, 22, 0.18);\n  --lx-accent-border: rgba(249, 115, 22, 0.28);\n\n  /* Status */\n  --lx-like: #f91880;\n  --lx-like-glow: rgba(249, 24, 128, 0.1);\n  --lx-reply: #1d9bf0;\n  --lx-reply-glow: rgba(29, 155, 240, 0.1);\n  --lx-danger: #ef4444;\n  --lx-danger-glow: rgba(239, 68, 68, 0.1);\n\n  font-family:\n    \"Plus Jakarta Sans\",\n    -apple-system,\n    sans-serif;\n  background-color: var(--lx-bg);\n  color: var(--lx-text);\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n}\n\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\n\nbody {\n  background: var(--lx-bg);\n  color: var(--lx-text);\n  min-height: 100vh;\n}\n\n/* Ambient orange glow from the left \u2014 brand presence */\nbody::before {\n  content: \"\";\n  position: fixed;\n  inset: 0;\n  background-image: radial-gradient(\n    ellipse 40% 70% at 0% 50%,\n    rgba(249, 115, 22, 0.045) 0%,\n    transparent 60%\n  );\n  pointer-events: none;\n  z-index: 0;\n}\n\n#root {\n  position: relative;\n  z-index: 1;\n}\n\n/* \u2500\u2500 Animations \u2500\u2500 */\n\n@keyframes fadeUp {\n  from {\n    opacity: 0;\n    transform: translateY(6px);\n  }\n  to {\n    opacity: 1;\n    transform: translateY(0);\n  }\n}\n\n@keyframes pulse-dot {\n  0%,\n  100% {\n    opacity: 0.5;\n  }\n  50% {\n    opacity: 1;\n  }\n}\n\n/* \u2500\u2500 Root Layout \u2500\u2500 */\n\n.lx-root {\n  display: flex;\n  max-width: 1280px;\n  margin: 0 auto;\n  min-height: 100vh;\n  animation: fadeUp 0.3s ease both;\n}\n\n/* \u2500\u2500 Left Sidebar \u2500\u2500 */\n\n.lx-sidebar-left {\n  width: 272px;\n  flex-shrink: 0;\n  position: sticky;\n  top: 0;\n  height: 100vh;\n  display: flex;\n  flex-direction: column;\n  padding: 0 0.75rem;\n  overflow-y: auto;\n  border-right: 1px solid var(--lx-border);\n}\n\n/* \u2500\u2500 Brand \u2500\u2500 */\n\n.lx-brand {\n  display: flex;\n  align-items: center;\n  gap: 0.55rem;\n  padding: 1.1rem 0.5rem 0.6rem;\n  cursor: pointer;\n}\n\n.lx-brand-logo {\n  width: 36px;\n  height: 36px;\n  object-fit: contain;\n  filter: drop-shadow(0 0 8px rgba(249, 115, 22, 0.4));\n}\n\n.lx-brand-name {\n  font-family: \"Poppins\", sans-serif;\n  font-size: 1.35rem;\n  font-weight: 800;\n  color: var(--lx-accent);\n  letter-spacing: -0.5px;\n  line-height: 1;\n}\n\n/* \u2500\u2500 Nav \u2500\u2500 */\n\n.lx-nav {\n  display: flex;\n  flex-direction: column;\n  gap: 0.1rem;\n  padding: 0.4rem 0 0.6rem;\n}\n\n.lx-nav-item {\n  display: flex;\n  align-items: center;\n  gap: 0.85rem;\n  padding: 0.7rem 0.9rem;\n  border-radius: 10px;\n  cursor: pointer;\n  font-size: 0.95rem;\n  font-weight: 500;\n  color: var(--lx-text-secondary);\n  transition:\n    background 0.15s,\n    color 0.15s;\n  user-select: none;\n  letter-spacing: -0.1px;\n}\n\n.lx-nav-item:hover {\n  background: var(--lx-bg-raised);\n  color: var(--lx-text);\n}\n\n.lx-nav-item-active {\n  font-weight: 700;\n  color: var(--lx-accent);\n  background: var(--lx-accent-glow);\n}\n\n.lx-nav-item-active:hover {\n  background: var(--lx-accent-glow2);\n  color: var(--lx-accent-light);\n}\n\n/* \u2500\u2500 Compose Button \u2500\u2500 */\n\n.lx-compose-btn {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  gap: 0.5rem;\n  width: calc(100% - 0.25rem);\n  margin: 0.75rem 0.125rem;\n  padding: 0.8rem 1.5rem;\n  background: linear-gradient(\n    135deg,\n    var(--lx-accent-light) 0%,\n    var(--lx-accent-dark) 100%\n  );\n  color: #fff;\n  border: none;\n  border-radius: 10px;\n  font-size: 0.925rem;\n  font-weight: 700;\n  cursor: pointer;\n  transition:\n    opacity 0.2s,\n    transform 0.15s,\n    box-shadow 0.2s;\n  font-family: \"Plus Jakarta Sans\", sans-serif;\n  letter-spacing: 0.1px;\n  box-shadow: 0 2px 16px rgba(249, 115, 22, 0.3);\n}\n\n.lx-compose-btn:hover {\n  opacity: 0.92;\n  transform: translateY(-1px);\n  box-shadow: 0 4px 20px rgba(249, 115, 22, 0.4);\n}\n\n.lx-compose-btn:active {\n  transform: translateY(0);\n}\n\n/* \u2500\u2500 Sidebar User \u2500\u2500 */\n\n.lx-sidebar-user {\n  display: flex;\n  align-items: center;\n  gap: 0.7rem;\n  padding: 0.75rem 0.6rem;\n  margin-top: auto;\n  margin-bottom: 0.75rem;\n  border-radius: 10px;\n  cursor: pointer;\n  transition: background 0.15s;\n  border: 1px solid var(--lx-border-subtle);\n}\n\n.lx-sidebar-user:hover {\n  background: var(--lx-bg-raised);\n  border-color: var(--lx-border);\n}\n\n.lx-sidebar-user-info {\n  flex: 1;\n  min-width: 0;\n}\n\n.lx-sidebar-user-name {\n  font-weight: 700;\n  font-size: 0.85rem;\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  color: var(--lx-text);\n}\n\n.lx-sidebar-user-handle {\n  color: var(--lx-text-muted);\n  font-size: 0.75rem;\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  margin-top: 0.05rem;\n}\n\n/* \u2500\u2500 Avatar \u2500\u2500 */\n\n.lx-avatar {\n  width: 40px;\n  height: 40px;\n  border-radius: 50%;\n  object-fit: cover;\n  flex-shrink: 0;\n  display: block;\n  border: 2px solid rgba(255, 255, 255, 0.06);\n  transition: border-color 0.2s;\n}\n\n.lx-avatar:hover {\n  border-color: var(--lx-accent-border);\n}\n\n.lx-avatar-sm {\n  width: 30px;\n  height: 30px;\n  border-radius: 50%;\n  object-fit: cover;\n  flex-shrink: 0;\n  display: block;\n  border: 1.5px solid rgba(255, 255, 255, 0.06);\n}\n\n.lx-avatar-lg {\n  width: 76px;\n  height: 76px;\n  border-radius: 50%;\n  object-fit: cover;\n  flex-shrink: 0;\n  display: block;\n  border: 3px solid var(--lx-bg);\n  outline: 2px solid var(--lx-accent-border);\n  position: relative;\n}\n\n/* \u2500\u2500 Main Column \u2500\u2500 */\n\n.lx-main {\n  flex: 1;\n  max-width: 598px;\n  border-right: 1px solid var(--lx-border);\n  min-height: 100vh;\n}\n\n/* \u2500\u2500 Feed Header \u2500\u2500 */\n\n.lx-feed-header {\n  position: sticky;\n  top: 0;\n  z-index: 100;\n  background: rgba(6, 6, 7, 0.82);\n  backdrop-filter: blur(16px) saturate(1.4);\n  -webkit-backdrop-filter: blur(16px) saturate(1.4);\n  border-bottom: 1px solid var(--lx-border);\n  padding: 0.9rem 1.1rem;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.lx-feed-title {\n  font-size: 1rem;\n  font-weight: 800;\n  letter-spacing: -0.2px;\n  font-family: \"Poppins\", sans-serif;\n  color: var(--lx-text);\n}\n\n/* \u2500\u2500 Composer \u2500\u2500 */\n\n.lx-composer {\n  display: flex;\n  gap: 0.75rem;\n  padding: 1rem 1.1rem;\n  border-bottom: 1px solid var(--lx-border);\n  background: var(--lx-bg-card);\n  transition: background 0.2s;\n}\n\n.lx-composer:focus-within {\n  background: var(--lx-bg);\n}\n\n.lx-composer:focus-within .lx-composer-footer {\n  border-top-color: var(--lx-border);\n}\n\n.lx-composer-textarea {\n  width: 100%;\n  background: transparent;\n  border: none;\n  color: var(--lx-text);\n  font-size: 1.05rem;\n  resize: none;\n  outline: none;\n  min-height: 72px;\n  font-family: \"Plus Jakarta Sans\", sans-serif;\n  line-height: 1.6;\n  letter-spacing: -0.1px;\n}\n\n.lx-composer-textarea::placeholder {\n  color: var(--lx-text-muted);\n}\n\n.lx-composer-body {\n  flex: 1;\n  display: flex;\n  flex-direction: column;\n  gap: 0.65rem;\n}\n\n.lx-composer-footer {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  border-top: 1px solid var(--lx-border-subtle);\n  padding-top: 0.65rem;\n  transition: border-color 0.2s;\n}\n\n.lx-char-ring-wrap {\n  display: flex;\n  align-items: center;\n  gap: 0.4rem;\n}\n\n.lx-char-ring {\n  width: 22px;\n  height: 22px;\n  flex-shrink: 0;\n}\n\n.lx-char-ring-bg {\n  fill: none;\n  stroke: rgba(255, 255, 255, 0.07);\n  stroke-width: 2.5;\n}\n\n.lx-char-ring-fill {\n  fill: none;\n  stroke-width: 2.5;\n  stroke-linecap: round;\n  transform: rotate(-90deg);\n  transform-origin: 12px 12px;\n  transition:\n    stroke-dashoffset 0.12s ease,\n    stroke 0.2s ease;\n}\n\n.lx-char-count {\n  font-size: 0.8rem;\n  color: var(--lx-text-muted);\n  font-variant-numeric: tabular-nums;\n  font-weight: 500;\n}\n\n.lx-char-count-warn {\n  font-size: 0.78rem;\n  color: var(--lx-accent);\n  font-variant-numeric: tabular-nums;\n  font-weight: 700;\n  min-width: 1.8rem;\n  text-align: right;\n}\n\n.lx-char-count-over {\n  font-size: 0.78rem;\n  color: var(--lx-danger);\n  font-variant-numeric: tabular-nums;\n  font-weight: 700;\n  min-width: 1.8rem;\n  text-align: right;\n}\n\n/* \u2500\u2500 Post Button \u2500\u2500 */\n\n.lx-post-btn {\n  background: linear-gradient(\n    135deg,\n    var(--lx-accent-light) 0%,\n    var(--lx-accent-dark) 100%\n  );\n  color: #fff;\n  border: none;\n  border-radius: 8px;\n  padding: 0.45rem 1.1rem;\n  font-size: 0.85rem;\n  font-weight: 700;\n  cursor: pointer;\n  transition:\n    opacity 0.2s,\n    transform 0.15s,\n    box-shadow 0.2s;\n  font-family: \"Plus Jakarta Sans\", sans-serif;\n  letter-spacing: 0.1px;\n  box-shadow: 0 1px 10px rgba(249, 115, 22, 0.25);\n}\n\n.lx-post-btn:hover {\n  opacity: 0.92;\n  transform: translateY(-1px);\n  box-shadow: 0 3px 14px rgba(249, 115, 22, 0.35);\n}\n\n.lx-post-btn:active {\n  transform: translateY(0);\n}\n\n.lx-post-btn:disabled {\n  opacity: 0.35;\n  cursor: not-allowed;\n  transform: none;\n  box-shadow: none;\n}\n\n/* \u2500\u2500 Tweet Card \u2500\u2500 */\n\n.lx-tweet {\n  padding: 1rem 1.1rem;\n  border-bottom: 1px solid var(--lx-border);\n  display: flex;\n  gap: 0.75rem;\n  transition: background 0.15s;\n  cursor: pointer;\n  position: relative;\n  animation: fadeUp 0.25s ease both;\n}\n\n.lx-tweet:hover {\n  background: var(--lx-bg-hover);\n}\n\n.lx-tweet:hover .lx-avatar {\n  border-color: var(--lx-accent-border);\n}\n\n.lx-tweet-body {\n  flex: 1;\n  min-width: 0;\n}\n\n.lx-tweet-header {\n  display: flex;\n  align-items: center;\n  gap: 0.3rem;\n  margin-bottom: 0.25rem;\n  flex-wrap: wrap;\n}\n\n.lx-tweet-name {\n  font-weight: 700;\n  font-size: 0.875rem;\n  color: var(--lx-text);\n  letter-spacing: -0.1px;\n}\n\n.lx-tweet-handle {\n  color: var(--lx-text-muted);\n  font-size: 0.825rem;\n}\n\n.lx-tweet-time {\n  color: var(--lx-text-muted);\n  font-size: 0.775rem;\n  font-variant-numeric: tabular-nums;\n}\n\n.lx-tweet-dot {\n  color: var(--lx-text-muted);\n  font-size: 0.75rem;\n}\n\n.lx-tweet-text {\n  font-size: 0.9375rem;\n  line-height: 1.65;\n  color: var(--lx-text);\n  margin-bottom: 0.65rem;\n  word-break: break-word;\n  letter-spacing: -0.1px;\n}\n\n/* \u2500\u2500 Tweet Actions \u2500\u2500 */\n\n.lx-tweet-actions {\n  display: flex;\n  align-items: center;\n  gap: 0;\n  margin-left: -0.5rem;\n}\n\n.lx-act-btn {\n  display: flex;\n  align-items: center;\n  gap: 0.3rem;\n  background: none;\n  border: none;\n  color: var(--lx-text-muted);\n  font-size: 0.775rem;\n  font-weight: 500;\n  cursor: pointer;\n  padding: 0.35rem 0.55rem;\n  border-radius: 8px;\n  transition:\n    color 0.15s,\n    background 0.15s;\n  font-family: \"Plus Jakarta Sans\", sans-serif;\n}\n\n.lx-act-comment:hover {\n  color: var(--lx-reply);\n  background: var(--lx-reply-glow);\n}\n\n.lx-act-like:hover {\n  color: var(--lx-like);\n  background: var(--lx-like-glow);\n}\n\n.lx-act-liked {\n  color: var(--lx-like);\n}\n\n.lx-act-liked:hover {\n  color: var(--lx-like);\n  background: var(--lx-like-glow);\n}\n\n.lx-act-delete:hover {\n  color: var(--lx-danger);\n  background: var(--lx-danger-glow);\n}\n\n/* \u2500\u2500 Comments Section \u2500\u2500 */\n\n.lx-comments-section {\n  margin-top: 0.65rem;\n  padding-top: 0.65rem;\n  border-top: 1px solid var(--lx-border);\n}\n\n.lx-comment-item {\n  display: flex;\n  gap: 0.55rem;\n  padding: 0.4rem 0;\n  align-items: flex-start;\n}\n\n.lx-comment-meta {\n  font-size: 0.775rem;\n  color: var(--lx-text-muted);\n  display: flex;\n  align-items: center;\n  gap: 0.25rem;\n  margin-bottom: 0.1rem;\n}\n\n.lx-comment-meta strong {\n  font-weight: 700;\n  color: var(--lx-text);\n  font-size: 0.825rem;\n}\n\n.lx-comment-text {\n  font-size: 0.85rem;\n  color: var(--lx-text-secondary);\n  line-height: 1.5;\n}\n\n.lx-comment-form {\n  display: flex;\n  gap: 0.5rem;\n  padding-top: 0.6rem;\n  align-items: center;\n}\n\n.lx-comment-input {\n  flex: 1;\n  background: var(--lx-bg-input);\n  border: 1px solid var(--lx-border);\n  border-radius: 8px;\n  color: var(--lx-text);\n  font-size: 0.825rem;\n  padding: 0.45rem 0.85rem;\n  outline: none;\n  font-family: \"Plus Jakarta Sans\", sans-serif;\n  transition:\n    border-color 0.2s,\n    background 0.2s;\n}\n\n.lx-comment-input:focus {\n  border-color: var(--lx-accent-border);\n  background: var(--lx-bg-card);\n}\n\n.lx-comment-input::placeholder {\n  color: var(--lx-text-muted);\n}\n\n.lx-comment-submit {\n  background: var(--lx-accent-glow2);\n  color: var(--lx-accent-light);\n  border: 1px solid var(--lx-accent-border);\n  border-radius: 8px;\n  padding: 0.4rem 0.85rem;\n  font-size: 0.78rem;\n  font-weight: 700;\n  cursor: pointer;\n  transition:\n    background 0.2s,\n    color 0.2s;\n  font-family: \"Plus Jakarta Sans\", sans-serif;\n  white-space: nowrap;\n}\n\n.lx-comment-submit:hover {\n  background: var(--lx-accent);\n  color: #fff;\n  border-color: transparent;\n}\n\n/* \u2500\u2500 Right Sidebar \u2500\u2500 */\n\n.lx-sidebar-right {\n  width: 340px;\n  flex-shrink: 0;\n  padding: 1rem 1.25rem 1rem 1.5rem;\n}\n\n.lx-sidebar-right-inner {\n  position: sticky;\n  top: 1rem;\n  display: flex;\n  flex-direction: column;\n  gap: 1rem;\n}\n\n/* \u2500\u2500 Search \u2500\u2500 */\n\n.lx-search-wrap {\n  position: relative;\n  display: flex;\n  gap: 0.5rem;\n  align-items: center;\n}\n\n.lx-search-icon {\n  position: absolute;\n  left: 0.9rem;\n  top: 50%;\n  transform: translateY(-50%);\n  color: var(--lx-text-muted);\n  pointer-events: none;\n  display: flex;\n  align-items: center;\n}\n\n.lx-search-input {\n  flex: 1;\n  min-width: 0;\n  background: var(--lx-bg-card);\n  border: 1px solid var(--lx-border);\n  border-radius: 10px;\n  color: var(--lx-text);\n  font-size: 0.875rem;\n  padding: 0.6rem 1.1rem 0.6rem 2.5rem;\n  outline: none;\n  font-family: \"Plus Jakarta Sans\", sans-serif;\n  transition:\n    border-color 0.2s,\n    background 0.2s,\n    box-shadow 0.2s;\n}\n\n.lx-search-input:focus {\n  border-color: var(--lx-accent-border);\n  background: var(--lx-bg-raised);\n  box-shadow: 0 0 0 3px var(--lx-accent-glow);\n}\n\n.lx-search-input::placeholder {\n  color: var(--lx-text-muted);\n}\n\n.lx-search-btn {\n  background: var(--lx-accent);\n  color: #fff;\n  border: none;\n  border-radius: 10px;\n  padding: 0.6rem 0.9rem;\n  font-size: 0.8rem;\n  font-weight: 700;\n  cursor: pointer;\n  font-family: \"Plus Jakarta Sans\", sans-serif;\n  white-space: nowrap;\n  flex-shrink: 0;\n  transition:\n    opacity 0.2s,\n    transform 0.15s;\n}\n\n.lx-search-btn:hover {\n  opacity: 0.88;\n  transform: translateY(-1px);\n}\n\n/* \u2500\u2500 Widget \u2500\u2500 */\n\n.lx-widget {\n  background: var(--lx-bg-card);\n  border: 1px solid var(--lx-border);\n  border-radius: 12px;\n  overflow: hidden;\n}\n\n.lx-widget-title {\n  font-size: 0.975rem;\n  font-weight: 800;\n  padding: 0.9rem 1rem 0.65rem;\n  font-family: \"Poppins\", sans-serif;\n  letter-spacing: -0.2px;\n  border-bottom: 1px solid var(--lx-border-subtle);\n}\n\n.lx-follow-item {\n  display: flex;\n  align-items: center;\n  gap: 0.6rem;\n  padding: 0.65rem 1rem;\n  transition: background 0.15s;\n  cursor: pointer;\n  border-bottom: 1px solid var(--lx-border-subtle);\n}\n\n.lx-follow-item:last-child {\n  border-bottom: none;\n}\n\n.lx-follow-item:hover {\n  background: var(--lx-bg-raised);\n}\n\n.lx-follow-item-info {\n  flex: 1;\n  min-width: 0;\n}\n\n.lx-follow-item-name {\n  font-weight: 700;\n  font-size: 0.825rem;\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  letter-spacing: -0.1px;\n}\n\n.lx-follow-item-handle {\n  color: var(--lx-text-muted);\n  font-size: 0.75rem;\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  margin-top: 0.05rem;\n}\n\n.lx-follow-btn {\n  background: var(--lx-text);\n  color: var(--lx-bg);\n  border: none;\n  border-radius: 7px;\n  padding: 0.32rem 0.8rem;\n  font-size: 0.775rem;\n  font-weight: 700;\n  cursor: pointer;\n  transition:\n    opacity 0.2s,\n    transform 0.15s;\n  white-space: nowrap;\n  font-family: \"Plus Jakarta Sans\", sans-serif;\n  letter-spacing: 0.1px;\n}\n\n.lx-follow-btn:hover {\n  opacity: 0.88;\n  transform: translateY(-1px);\n}\n\n.lx-unfollow-btn {\n  background: transparent;\n  color: var(--lx-text-secondary);\n  border: 1px solid var(--lx-border-strong);\n  border-radius: 7px;\n  padding: 0.32rem 0.8rem;\n  font-size: 0.775rem;\n  font-weight: 700;\n  cursor: pointer;\n  transition:\n    color 0.2s,\n    border-color 0.2s,\n    background 0.2s;\n  white-space: nowrap;\n  font-family: \"Plus Jakarta Sans\", sans-serif;\n}\n\n.lx-unfollow-btn:hover {\n  color: var(--lx-danger);\n  border-color: rgba(239, 68, 68, 0.4);\n  background: var(--lx-danger-glow);\n}\n\n.lx-widget-show-more {\n  padding: 0.75rem 1rem;\n  color: var(--lx-accent);\n  font-size: 0.825rem;\n  font-weight: 600;\n  cursor: pointer;\n  transition:\n    color 0.15s,\n    background 0.15s;\n}\n\n.lx-widget-show-more:hover {\n  color: var(--lx-accent-light);\n  background: var(--lx-accent-glow);\n}\n\n/* \u2500\u2500 Explore Item \u2500\u2500 */\n\n.lx-explore-item {\n  display: flex;\n  align-items: center;\n  gap: 0.75rem;\n  padding: 0.9rem 1.1rem;\n  border-bottom: 1px solid var(--lx-border);\n  transition: background 0.15s;\n  cursor: pointer;\n  animation: fadeUp 0.2s ease both;\n}\n\n.lx-explore-item:hover {\n  background: var(--lx-bg-hover);\n}\n\n.lx-explore-info {\n  flex: 1;\n  min-width: 0;\n}\n\n.lx-explore-name {\n  font-weight: 700;\n  font-size: 0.875rem;\n  color: var(--lx-text);\n  letter-spacing: -0.1px;\n}\n\n.lx-explore-handle {\n  color: var(--lx-text-muted);\n  font-size: 0.775rem;\n  margin-top: 0.05rem;\n}\n\n.lx-explore-bio {\n  color: var(--lx-text-secondary);\n  font-size: 0.8rem;\n  margin-top: 0.2rem;\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n}\n\n/* \u2500\u2500 Profile \u2500\u2500 */\n\n.lx-profile-banner {\n  height: 130px;\n  background: linear-gradient(160deg, #0e0e12 0%, #141018 50%, #1c1008 100%);\n  position: relative;\n  overflow: hidden;\n}\n\n.lx-profile-banner::after {\n  content: \"\";\n  position: absolute;\n  inset: 0;\n  background: radial-gradient(\n    ellipse 55% 120% at 90% 100%,\n    rgba(249, 115, 22, 0.18) 0%,\n    transparent 65%\n  );\n}\n\n.lx-profile-info {\n  padding: 0 1.1rem 1.25rem;\n}\n\n.lx-profile-avatar-wrap {\n  margin-top: -2.75rem;\n  margin-bottom: 0.85rem;\n}\n\n.lx-profile-display-name {\n  font-size: 1.1rem;\n  font-weight: 800;\n  font-family: \"Poppins\", sans-serif;\n  color: var(--lx-text);\n  letter-spacing: -0.3px;\n}\n\n.lx-profile-handle {\n  color: var(--lx-text-muted);\n  font-size: 0.825rem;\n  margin-top: 0.1rem;\n}\n\n.lx-profile-bio-text {\n  font-size: 0.875rem;\n  color: var(--lx-text-secondary);\n  margin-top: 0.65rem;\n  line-height: 1.55;\n}\n\n.lx-profile-stats {\n  display: flex;\n  gap: 1.5rem;\n  margin-top: 0.85rem;\n  flex-wrap: wrap;\n}\n\n.lx-profile-stat {\n  font-size: 0.825rem;\n  color: var(--lx-text-muted);\n}\n\n.lx-profile-stat strong {\n  font-weight: 800;\n  color: var(--lx-text);\n  margin-right: 0.2rem;\n  font-size: 0.9rem;\n}\n\n.lx-edit-profile-btn {\n  background: transparent;\n  color: var(--lx-text-secondary);\n  border: 1px solid var(--lx-border-strong);\n  border-radius: 8px;\n  padding: 0.4rem 1rem;\n  font-size: 0.825rem;\n  font-weight: 700;\n  cursor: pointer;\n  transition:\n    background 0.2s,\n    color 0.2s,\n    border-color 0.2s;\n  font-family: \"Plus Jakarta Sans\", sans-serif;\n  letter-spacing: 0.1px;\n}\n\n.lx-edit-profile-btn:hover {\n  background: var(--lx-bg-raised);\n  color: var(--lx-text);\n  border-color: var(--lx-border-strong);\n}\n\n.lx-bio-edit-wrap {\n  display: flex;\n  flex-direction: column;\n  gap: 0.5rem;\n  margin-top: 0.75rem;\n}\n\n.lx-bio-input {\n  background: var(--lx-bg-input);\n  border: 1px solid var(--lx-border);\n  border-radius: 8px;\n  color: var(--lx-text);\n  font-size: 0.875rem;\n  padding: 0.6rem 0.85rem;\n  outline: none;\n  resize: none;\n  font-family: \"Plus Jakarta Sans\", sans-serif;\n  width: 100%;\n  transition:\n    border-color 0.2s,\n    box-shadow 0.2s;\n  line-height: 1.5;\n}\n\n.lx-bio-input:focus {\n  border-color: var(--lx-accent-border);\n  box-shadow: 0 0 0 3px var(--lx-accent-glow);\n}\n\n.lx-bio-input::placeholder {\n  color: var(--lx-text-muted);\n}\n\n.lx-bio-save-btn {\n  align-self: flex-start;\n  background: linear-gradient(\n    135deg,\n    var(--lx-accent-light) 0%,\n    var(--lx-accent-dark) 100%\n  );\n  color: #fff;\n  border: none;\n  border-radius: 8px;\n  padding: 0.4rem 1rem;\n  font-size: 0.8rem;\n  font-weight: 700;\n  cursor: pointer;\n  transition:\n    opacity 0.2s,\n    transform 0.15s;\n  font-family: \"Plus Jakarta Sans\", sans-serif;\n  box-shadow: 0 1px 8px rgba(249, 115, 22, 0.25);\n}\n\n.lx-bio-save-btn:hover {\n  opacity: 0.9;\n  transform: translateY(-1px);\n}\n\n/* \u2500\u2500 Auth \u2500\u2500 */\n\n.lx-auth-root {\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  min-height: 100vh;\n}\n\n.lx-auth-left {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  background: var(--lx-bg-subtle);\n  border-right: 1px solid var(--lx-border);\n  position: relative;\n  overflow: hidden;\n}\n\n.lx-auth-left::before {\n  content: \"\";\n  position: absolute;\n  inset: 0;\n  background: radial-gradient(\n    ellipse 70% 70% at 50% 50%,\n    rgba(249, 115, 22, 0.09) 0%,\n    transparent 70%\n  );\n}\n\n.lx-auth-logo-big {\n  width: 160px;\n  height: 160px;\n  object-fit: contain;\n  position: relative;\n  z-index: 1;\n  filter: drop-shadow(0 0 40px rgba(249, 115, 22, 0.35));\n  animation: fadeUp 0.4s ease both;\n}\n\n.lx-auth-right {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  padding: 2rem;\n  background: var(--lx-bg);\n}\n\n.lx-auth-box {\n  width: 100%;\n  max-width: 360px;\n  display: flex;\n  flex-direction: column;\n  gap: 1rem;\n  animation: fadeUp 0.35s 0.05s ease both;\n}\n\n.lx-auth-heading {\n  font-family: \"Poppins\", sans-serif;\n  font-size: 2.4rem;\n  font-weight: 800;\n  color: var(--lx-accent);\n  letter-spacing: -1.5px;\n  line-height: 1;\n}\n\n.lx-auth-sub {\n  color: var(--lx-text-muted);\n  font-size: 0.9rem;\n  margin-top: -0.2rem;\n  line-height: 1.5;\n}\n\n.lx-auth-error {\n  background: rgba(239, 68, 68, 0.08);\n  border: 1px solid rgba(239, 68, 68, 0.3);\n  color: var(--lx-danger);\n  border-radius: 8px;\n  padding: 0.7rem 1rem;\n  font-size: 0.85rem;\n  font-weight: 500;\n}\n\n.lx-input {\n  width: 100%;\n  background: var(--lx-bg-card);\n  border: 1px solid var(--lx-border);\n  border-radius: 9px;\n  color: var(--lx-text);\n  font-size: 0.9375rem;\n  padding: 0.75rem 1rem;\n  outline: none;\n  transition:\n    border-color 0.2s,\n    background 0.2s,\n    box-shadow 0.2s;\n  font-family: \"Plus Jakarta Sans\", sans-serif;\n}\n\n.lx-input:focus {\n  border-color: var(--lx-accent-border);\n  background: var(--lx-bg-raised);\n  box-shadow: 0 0 0 3px var(--lx-accent-glow);\n}\n\n.lx-input::placeholder {\n  color: var(--lx-text-muted);\n}\n\n.lx-auth-btn {\n  width: 100%;\n  padding: 0.8rem;\n  background: linear-gradient(\n    135deg,\n    var(--lx-accent-light) 0%,\n    var(--lx-accent-dark) 100%\n  );\n  color: #fff;\n  border: none;\n  border-radius: 9px;\n  font-size: 0.95rem;\n  font-weight: 700;\n  cursor: pointer;\n  transition:\n    opacity 0.2s,\n    transform 0.15s,\n    box-shadow 0.2s;\n  font-family: \"Plus Jakarta Sans\", sans-serif;\n  letter-spacing: 0.1px;\n  box-shadow: 0 2px 16px rgba(249, 115, 22, 0.28);\n}\n\n.lx-auth-btn:hover {\n  opacity: 0.92;\n  transform: translateY(-1px);\n  box-shadow: 0 4px 20px rgba(249, 115, 22, 0.38);\n}\n\n.lx-auth-btn:active {\n  transform: translateY(0);\n}\n\n.lx-auth-btn:disabled {\n  opacity: 0.4;\n  cursor: not-allowed;\n  transform: none;\n  box-shadow: none;\n}\n\n.lx-auth-toggle {\n  text-align: center;\n  color: var(--lx-text-muted);\n  font-size: 0.85rem;\n}\n\n.lx-auth-toggle-link {\n  color: var(--lx-accent);\n  cursor: pointer;\n  font-weight: 600;\n  transition: color 0.15s;\n}\n\n.lx-auth-toggle-link:hover {\n  color: var(--lx-accent-light);\n}\n\n.lx-auth-form-fields {\n  display: flex;\n  flex-direction: column;\n  gap: 0.7rem;\n}\n\n/* \u2500\u2500 Utility \u2500\u2500 */\n\n.lx-empty {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  padding: 4rem 1rem;\n  text-align: center;\n  color: var(--lx-text-muted);\n  gap: 0.4rem;\n}\n\n.lx-empty-title {\n  font-size: 1.15rem;\n  font-weight: 800;\n  color: var(--lx-text-secondary);\n  font-family: \"Poppins\", sans-serif;\n  letter-spacing: -0.3px;\n}\n\n.lx-loading {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  padding: 2.5rem 1rem;\n  color: var(--lx-text-muted);\n  font-size: 0.85rem;\n  gap: 0.5rem;\n}\n\n.lx-loading::before {\n  content: \"\";\n  width: 6px;\n  height: 6px;\n  border-radius: 50%;\n  background: var(--lx-accent);\n  animation: pulse-dot 1.2s ease-in-out infinite;\n}\n\n.lx-divider {\n  height: 1px;\n  background: var(--lx-border);\n  border: none;\n}\n\n.lx-logout-icon-btn {\n  background: transparent;\n  border: none;\n  color: var(--lx-text-muted);\n  cursor: pointer;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  padding: 0.35rem;\n  border-radius: 7px;\n  transition:\n    color 0.2s,\n    background 0.2s;\n  flex-shrink: 0;\n}\n\n.lx-logout-icon-btn:hover {\n  color: var(--lx-danger);\n  background: var(--lx-danger-glow);\n}\n\n/* \u2500\u2500 Repost action \u2500\u2500 */\n\n.lx-act-repost:hover {\n  color: #22c55e;\n  background: rgba(34, 197, 94, 0.1);\n}\n\n/* \u2500\u2500 Trending Widget \u2500\u2500 */\n\n.lx-trend-item {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0.6rem 1rem;\n  cursor: pointer;\n  transition: background 0.15s;\n  border-bottom: 1px solid var(--lx-border-subtle);\n}\n\n.lx-trend-item:last-child {\n  border-bottom: none;\n}\n\n.lx-trend-item:hover {\n  background: var(--lx-bg-raised);\n}\n\n.lx-trend-tag {\n  font-size: 0.85rem;\n  font-weight: 700;\n  color: var(--lx-accent);\n  letter-spacing: -0.1px;\n}\n\n.lx-trend-count {\n  font-size: 0.72rem;\n  color: var(--lx-text-muted);\n  font-variant-numeric: tabular-nums;\n}\n\n/* \u2500\u2500 Notification Badge \u2500\u2500 */\n\n.lx-nav-badge {\n  background: var(--lx-accent);\n  color: #fff;\n  border-radius: 9999px;\n  font-size: 0.65rem;\n  font-weight: 800;\n  min-width: 18px;\n  height: 18px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  padding: 0 5px;\n  margin-left: auto;\n  animation: fadeUp 0.2s ease both;\n}\n\n/* \u2500\u2500 Welcome Banner \u2500\u2500 */\n\n.lx-welcome-banner {\n  display: flex;\n  align-items: flex-start;\n  justify-content: space-between;\n  gap: 1rem;\n  padding: 1rem 1.1rem;\n  background: linear-gradient(\n    135deg,\n    rgba(249, 115, 22, 0.08) 0%,\n    rgba(249, 115, 22, 0.04) 100%\n  );\n  border-bottom: 1px solid var(--lx-accent-border);\n  animation: fadeUp 0.3s ease both;\n}\n\n.lx-welcome-content {\n  display: flex;\n  flex-direction: column;\n  gap: 0.5rem;\n}\n\n.lx-welcome-title {\n  font-family: \"Poppins\", sans-serif;\n  font-size: 1rem;\n  font-weight: 800;\n  color: var(--lx-accent);\n  letter-spacing: -0.2px;\n}\n\n.lx-welcome-tips {\n  display: flex;\n  flex-direction: column;\n  gap: 0.2rem;\n}\n\n.lx-welcome-tip {\n  font-size: 0.8rem;\n  color: var(--lx-text-secondary);\n  display: flex;\n  align-items: center;\n  gap: 0.4rem;\n}\n\n.lx-welcome-tip::before {\n  content: \"\";\n  width: 4px;\n  height: 4px;\n  border-radius: 50%;\n  background: var(--lx-accent);\n  flex-shrink: 0;\n}\n\n.lx-welcome-dismiss {\n  background: var(--lx-accent);\n  color: #fff;\n  border: none;\n  border-radius: 8px;\n  padding: 0.4rem 0.9rem;\n  font-size: 0.8rem;\n  font-weight: 700;\n  cursor: pointer;\n  font-family: \"Plus Jakarta Sans\", sans-serif;\n  white-space: nowrap;\n  flex-shrink: 0;\n  transition: opacity 0.2s;\n}\n\n.lx-welcome-dismiss:hover {\n  opacity: 0.85;\n}\n\n/* \u2500\u2500 About Widget \u2500\u2500 */\n\n.lx-about-widget {\n  background: var(--lx-bg-card);\n  border: 1px solid var(--lx-border);\n  border-radius: 12px;\n  padding: 1rem;\n  display: flex;\n  flex-direction: column;\n  gap: 0.5rem;\n}\n\n.lx-about-title {\n  font-family: \"Poppins\", sans-serif;\n  font-size: 0.975rem;\n  font-weight: 800;\n  color: var(--lx-accent);\n  letter-spacing: -0.2px;\n}\n\n.lx-about-desc {\n  font-size: 0.8rem;\n  color: var(--lx-text-secondary);\n  line-height: 1.55;\n}\n\n.lx-about-links {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 0.4rem;\n  margin-top: 0.25rem;\n}\n\n.lx-about-tag {\n  font-size: 0.75rem;\n  font-weight: 600;\n  color: var(--lx-accent);\n  background: var(--lx-accent-glow);\n  border: 1px solid var(--lx-accent-border);\n  border-radius: 6px;\n  padding: 0.2rem 0.55rem;\n  cursor: pointer;\n  transition: background 0.15s;\n}\n\n.lx-about-tag:hover {\n  background: var(--lx-accent-glow2);\n}\n\n/* \u2500\u2500 Scrollbar \u2500\u2500 */\n\n::-webkit-scrollbar {\n  width: 4px;\n}\n\n::-webkit-scrollbar-track {\n  background: transparent;\n}\n\n::-webkit-scrollbar-thumb {\n  background: rgba(255, 255, 255, 0.08);\n  border-radius: 4px;\n}\n\n::-webkit-scrollbar-thumb:hover {\n  background: rgba(255, 255, 255, 0.15);\n}\n\n/* \u2500\u2500 Mobile Navigation (hidden on desktop) \u2500\u2500 */\n\n.lx-mobile-nav {\n  display: none;\n}\n\n.lx-mobile-fab {\n  display: none;\n}\n\n.lx-mobile-nav-item {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  padding: 0.55rem 1.5rem;\n  border-radius: 10px;\n  cursor: pointer;\n  color: var(--lx-text-muted);\n  transition:\n    color 0.15s,\n    background 0.15s;\n  position: relative;\n  user-select: none;\n}\n\n.lx-mobile-nav-item:hover {\n  color: var(--lx-text);\n}\n\n.lx-mobile-nav-item-active {\n  color: var(--lx-accent);\n}\n\n.lx-mobile-nav-item .lx-nav-badge {\n  position: absolute;\n  top: 4px;\n  right: 14px;\n  font-size: 0.6rem;\n  min-width: 16px;\n  height: 16px;\n  margin-left: 0;\n}\n\n/* \u2500\u2500 Responsive: Tablet (768px \u2013 1099px) \u2500\u2500 */\n\n@media (max-width: 1099px) {\n  .lx-sidebar-right {\n    display: none;\n  }\n\n  .lx-main {\n    max-width: none;\n    flex: 1;\n    border-right: none;\n    min-height: 94vh;\n  }\n}\n\n@media (min-width: 768px) and (max-width: 1099px) {\n  .lx-sidebar-left {\n    width: 72px;\n    padding: 0 0.25rem;\n    align-items: center;\n    border-right: 1px solid var(--lx-border);\n  }\n\n  .lx-brand {\n    justify-content: center;\n    padding: 1.1rem 0.25rem 0.6rem;\n  }\n\n  .lx-brand-name {\n    display: none;\n  }\n\n  .lx-nav-item {\n    justify-content: center;\n    padding: 0.8rem;\n    position: relative;\n  }\n\n  .lx-nav-item span:not(.lx-nav-badge) {\n    display: none;\n  }\n\n  .lx-nav-badge {\n    position: absolute;\n    top: 4px;\n    right: 4px;\n    margin-left: 0;\n    font-size: 0.6rem;\n    min-width: 16px;\n    height: 16px;\n  }\n\n  .lx-compose-btn {\n    width: 46px;\n    height: 46px;\n    padding: 0;\n    border-radius: 50%;\n    margin: 0.75rem auto;\n  }\n\n  .lx-compose-btn span {\n    display: none;\n  }\n\n  .lx-sidebar-user {\n    justify-content: center;\n    padding: 0.65rem;\n  }\n\n  .lx-sidebar-user-info {\n    display: none;\n  }\n\n  .lx-logout-icon-btn {\n    display: none;\n  }\n}\n\n/* \u2500\u2500 Responsive: Mobile (< 768px) \u2500\u2500 */\n\n@media (max-width: 767px) {\n  .lx-root {\n    padding-bottom: calc(56px + env(safe-area-inset-bottom, 0px));\n  }\n\n  .lx-sidebar-left {\n    display: none;\n  }\n\n  .lx-main {\n    width: 100%;\n    max-width: 100%;\n    border-right: none;\n    min-height: 94vh;\n  }\n\n  /* Bottom navigation bar */\n  .lx-mobile-nav {\n    display: flex;\n    position: fixed;\n    bottom: 0;\n    left: 0;\n    right: 0;\n    height: calc(56px + env(safe-area-inset-bottom, 0px));\n    padding-bottom: env(safe-area-inset-bottom, 0px);\n    background: rgba(6, 6, 7, 0.95);\n    backdrop-filter: blur(16px) saturate(1.4);\n    -webkit-backdrop-filter: blur(16px) saturate(1.4);\n    border-top: 1px solid var(--lx-border);\n    z-index: 200;\n    align-items: center;\n    justify-content: space-around;\n    padding-left: 0.5rem;\n    padding-right: 0.5rem;\n  }\n\n  /* Floating action button for compose */\n  .lx-mobile-fab {\n    display: flex;\n    position: fixed;\n    bottom: calc(4.5rem + env(safe-area-inset-bottom, 0px));\n    right: 1rem;\n    width: 52px;\n    height: 52px;\n    border-radius: 50%;\n    background: linear-gradient(\n      135deg,\n      var(--lx-accent-light) 0%,\n      var(--lx-accent-dark) 100%\n    );\n    color: #fff;\n    border: none;\n    cursor: pointer;\n    align-items: center;\n    justify-content: center;\n    box-shadow: 0 4px 20px rgba(249, 115, 22, 0.45);\n    z-index: 200;\n    transition:\n      transform 0.2s,\n      box-shadow 0.2s;\n    font-family: \"Plus Jakarta Sans\", sans-serif;\n  }\n\n  .lx-mobile-fab:hover {\n    transform: scale(1.08);\n    box-shadow: 0 6px 24px rgba(249, 115, 22, 0.55);\n  }\n\n  .lx-mobile-fab:active {\n    transform: scale(0.94);\n  }\n\n  /* Auth page: single column */\n  .lx-auth-root {\n    grid-template-columns: 1fr;\n  }\n\n  .lx-auth-left {\n    display: none;\n  }\n\n  .lx-auth-right {\n    padding: 2rem 1.25rem;\n    align-items: flex-start;\n    padding-top: 3rem;\n  }\n\n  .lx-auth-heading {\n    font-size: 2rem;\n  }\n\n  /* Tighter padding on mobile */\n  .lx-feed-header {\n    padding: 0.75rem 0.9rem;\n  }\n\n  .lx-composer {\n    padding: 0.85rem 0.9rem;\n  }\n\n  .lx-tweet {\n    padding: 0.85rem 0.9rem;\n  }\n\n  .lx-profile-info {\n    padding: 0 0.9rem 1rem;\n  }\n\n  .lx-explore-item {\n    padding: 0.75rem 0.9rem;\n  }\n\n  /* Composer textarea smaller on mobile */\n  .lx-composer-textarea {\n    font-size: 0.975rem;\n    min-height: 60px;\n  }\n\n  /* Profile banner shorter on mobile */\n  .lx-profile-banner {\n    height: 100px;\n  }\n\n  /* Welcome banner wraps gracefully */\n  .lx-welcome-banner {\n    flex-direction: column;\n    gap: 0.75rem;\n    padding: 0.85rem 0.9rem;\n  }\n\n  .lx-welcome-dismiss {\n    align-self: flex-start;\n  }\n\n  /* Tweet actions fit on smaller screens */\n  .lx-act-btn {\n    padding: 0.3rem 0.4rem;\n    font-size: 0.725rem;\n  }\n\n  /* Comment form stacks better */\n  .lx-comment-form {\n    flex-wrap: wrap;\n    gap: 0.4rem;\n  }\n\n  .lx-comment-input {\n    flex: 1 1 100%;\n  }\n\n  .lx-comment-submit {\n    align-self: flex-end;\n  }\n}\n",
